{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Presentation.Api/Presentation.Api.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Presentation.Api/Presentation.Api.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/Presentation.Api/Presentation.Api.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
          "label": "test",
          "command": "dotnet",
          "type": "process",
          "args": [
              "test",
              "/p:CollectCoverage=true"
          ],
          "problemMatcher": "$tsc"
      }
    ]
}